var URI = {
  getParameters:function() {
      var map = new Object();
      var kvs = window.location.search.split('?');
      if (kvs.length > 0) {
        kvs = kvs[1].split('&');
        for (var i = 0; i < kvs.length; i++) {
          var kv = kvs[i].split('=');
          map[kv[0]] = decodeURIComponent(kv[1]);
        }
      }
      return map;
  }
};
